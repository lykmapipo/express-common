{
  "name": "@lykmapipo/express-common",
  "version": "0.18.14",
  "description": "minimal express app configuration",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "clean": "rimraf lib dist es umd logs",
    "prebuild": "npm test && npm run clean",
    "build": "rollup -c",
    "lint": "eslint --fix --ext .js src/ test/ rollup.config.js",
    "pretest": "npm run lint",
    "test": "NODE_ENV=test mocha --require @babel/register test/**/*.spec.js",
    "docs": "doxdox 'lib/**/*.js' -p package.json -l markdown -o DOCUMENTATION.md",
    "dev": "BASE_PATH=./examples node examples/app.js",
    "cmt": "git add -A && git-cz",
    "changelog": "changelog",
    "chore:deps": "ncu -u && npm install --save"
  },
  "keywords": [
    "lykmapipo",
    "express",
    "express-common",
    "starter",
    "common",
    "rapid",
    "sinatra",
    "rest",
    "restify",
    "api",
    "helmet",
    "cors",
    "compression",
    "body-parser",
    "morgan",
    "dotenv",
    "mquery"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lykmapipo/express-common.git"
  },
  "author": {
    "name": "lykmapipo",
    "email": "lallyelias87@gmail.com",
    "url": "https://github.com/lykmapipo"
  },
  "contributors": [
    {
      "name": "lykmapipo",
      "email": "lallyelias87@gmail.com",
      "url": "https://github.com/lykmapipo"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lykmapipo/express-common/issues"
  },
  "homepage": "https://github.com/lykmapipo/express-common#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/travis-cli": "^11.0.0",
    "@lykmapipo/test-helpers": "^0.6.6",
    "chai": "^4.2.0",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "dot-prop": "^6.0.1",
    "doxdox": "^3.0.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "generate-changelog": "^1.8.0",
    "handlebars": "^4.7.6",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "lodash.template": "^4.5.0",
    "minimist": "^1.2.5",
    "mocha": "^8.2.1",
    "npm-check-updates": "^10.2.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.35.1",
    "shelljs": "^0.8.4",
    "supertest": ">=6.0.1",
    "websocket-extensions": "^0.1.4"
  },
  "peerDependencies": {},
  "dependencies": {
    "@lykmapipo/common": ">=0.42.4",
    "@lykmapipo/env": ">=0.17.24",
    "@lykmapipo/express-request-extra": ">=0.1.40",
    "@lykmapipo/express-router-extra": ">=0.5.15",
    "@lykmapipo/logger": ">=0.5.32",
    "async": ">=3.2.0",
    "body-parser": ">=1.19.0",
    "compression": ">=1.7.4",
    "cors": ">=2.8.5",
    "express": ">=4.17.1",
    "express-mquery": ">=1.2.13",
    "express-respond": ">=1.5.14",
    "helmet": ">=4.2.0",
    "lodash": ">=4.17.20",
    "method-override": ">=3.0.0",
    "morgan": ">=1.10.0",
    "serve-favicon": ">=2.5.0",
    "uuid": ">=8.3.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint",
      "git add -A"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
